#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/reactjs/coding-practices/feedbackApp/backend/node_modules/.pnpm/kill-port@2.0.1/node_modules/kill-port/node_modules:/home/workspace/reactjs/coding-practices/feedbackApp/backend/node_modules/.pnpm/kill-port@2.0.1/node_modules:/home/workspace/reactjs/coding-practices/feedbackApp/backend/node_modules/.pnpm/node_modules:/home/workspace/reactjs/coding-practices/feedbackApp/backend/node_modules:/home/workspace/reactjs/coding-practices/feedbackApp/node_modules:/home/workspace/reactjs/coding-practices/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/coding-practices/feedbackApp/backend/node_modules/kill-port/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/reactjs/coding-practices/feedbackApp/backend/node_modules/.pnpm/kill-port@2.0.1/node_modules/kill-port/node_modules:/home/workspace/reactjs/coding-practices/feedbackApp/backend/node_modules/.pnpm/kill-port@2.0.1/node_modules:/home/workspace/reactjs/coding-practices/feedbackApp/backend/node_modules/.pnpm/node_modules:/home/workspace/reactjs/coding-practices/feedbackApp/backend/node_modules:/home/workspace/reactjs/coding-practices/feedbackApp/node_modules:/home/workspace/reactjs/coding-practices/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/coding-practices/feedbackApp/backend/node_modules/kill-port/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../kill-port/cli.js" "$@"
else
  exec node  "$basedir/../kill-port/cli.js" "$@"
fi
